    "python-shell": "^1.0.7",
    "react-voice-components": "^1.0.0"
  },
  "devDependencies": {
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "concurrently": "^3.5.1",
    "firebase": "^4.13.1",
    "react-scripts": "0.6.1"
  }
}
